{
  "version": 2,
  "env": {
    "FORCE_REBUILD": "1724440200",
    "NODE_ENV": "production"
  },
  "builds": [
    { "src": "api/**/*.js", "use": "@vercel/node" },
    { "src": "public/**", "use": "@vercel/static" },
    { "src": "refs/out/**", "use": "@vercel/static" },
    { "src": "lib/**", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/refs/out/(.*)", "dest": "/public/refs/out/$1" },
    { "src": "/refs/(.*)", "dest": "/public/refs/$1" },
    { "src": "/lib/(.*)", "dest": "/lib/$1" },
    { "src": "/api/(.*)", "dest": "api/$1.js" },
    { "src": "/", "dest": "/public/landing.html" },
    { "src": "/(.*)", "dest": "/public/$1" }
  ],
  "headers": [
    {
      "source": "/refs/embedded-refs.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/public/refs/embedded-refs.js",
      "headers": [
        {
          "key": "Cache-Control", 
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}